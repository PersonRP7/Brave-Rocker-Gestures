{
    "manifest_version": 3,
    "name": "Rocker Gestures",
    "version": "1.0",
    "description": "Simple back/forward navigation with rocker gestures",
    "permissions": ["scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ]
  }
