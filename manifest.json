{
    "manifest_version": 3,
    "name": "Forwards / Backwards Rocker Gestures",
    "version": "0.0.0.1",
    "description": "Mouse based navigation. In rapid succession press right click then left click to go backwards and reverse to go forwards.",
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": ["scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ],
    "action": {
    "default_popup": "hello.html",
    "default_icon": "icon128.png"
  }
  }
